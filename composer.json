    "homepage": "https://github.com/nfephp-org/sped-esfinge",
    "license": ["GPL-3.0+","LGPL-3.0+", "MIT"],
    "authors": [
        {
            "name": "Roberto L. Machado",
            "email": "linux.rlm@gmail.com.br",
            "homepage": "http://www.nfephp.org",
            "role": "Developer"
        },
        {
            "name": "Comunidade NFePHP",
            "homepage": "https://github.com/nfephp-org/sped-esfinge/graphs/contributors"
